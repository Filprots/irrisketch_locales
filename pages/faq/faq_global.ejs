<% if (locale === 'es') { %>
    <%- partial('./es/es.ejs') %>
<% }  else if (locale === 'fr') { %>
    <%- partial('./fr/fr.ejs') %>
<% }  else  if (locale === 'lt') { %>
    <%- partial('./lt/lt.ejs') %>
<% }  else  if (locale === 'nl') { %>
    <%- partial('./nl/nl.ejs') %>
<% }  else  if (locale === 'it') { %>
    <%- partial('./it/it.ejs') %>
<% }  else  if (locale === 'de') { %>
    <%- partial('./de/de.ejs') %>
<% }  else  if (locale === 'ru') { %>
    <%- partial('./ru/ru.ejs') %>
<% } else  if (locale === 'hr') { %>
    <%- partial('./hr/hr.ejs') %>
<% } else { %>
    <%- partial('./en/en.ejs') %>
<% } %>
